n <- 100
# Contoh white noise stasioner
y_stationary <- rnorm(n, mean = 10, sd = 2)
# Atau contoh AR(1) stasioner
phi <- 0.5
y_ar1 <- numeric(n)
y_ar1[1] <- rnorm(1, 10, 2)
for(i in 2:n){
y_ar1[i] <- phi * y_ar1[i-1] + rnorm(1, 0, 1)
}
# Data frame
df_stationary <- data.frame(
Time = 1:n,
Sales = y_ar1
)
# Plot data
plot(ts(df_stationary$Sales), type="l", col="darkgreen", lwd=2, main="Contoh Data Stasioner (AR(1))")
# Jalankan ants_arima
