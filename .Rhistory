# 3. Exponential
data_exp <- rexp(100, rate = 1.5)
# 4. Gamma
data_gamma <- rgamma(100, shape = 2, rate = 0.5)
# 5. Lognormal
data_lnorm <- rlnorm(100, meanlog = 1, sdlog = 0.5)
# 6. Uniform
data_unif <- runif(100, min = 0, max = 10)
# 7. Weibull
data_weibull <- rweibull(100, shape = 2, scale = 1)
# 8. Cauchy
data_cauchy <- rcauchy(100, location = 0, scale = 1)
# 9. t-distribution
data_t <- rt(100, df = 5)
# 10. F-distribution
data_f <- rf(100, df1 = 5, df2 = 10)
# Contoh: gabungkan semua menjadi list
data_list <- list(
Normal = data_norm,
Poisson = data_pois,
Exponential = data_exp,
Gamma = data_gamma,
Lognormal = data_lnorm,
Uniform = data_unif,
Weibull = data_weibull,
Cauchy = data_cauchy,
t = data_t,
F = data_f
)
res <- detect_dist(data_list$Normal)
print(res$best_distribution)
print(res$best_parameters)
